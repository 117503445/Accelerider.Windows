<Window x:Class="Accelerider.Windows.Views.Dialogs.AuthenticationBrowserWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="AuthenticationBrowserWindow" Height="550" Width="600"
        WindowStartupLocation="CenterScreen" ResizeMode="CanResize"
        Style="{StaticResource EnteringWindowStyle}">
    <Grid Margin="1 35 1 1">
        <UserControl Content="{Binding Browser, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=Window}}"/>
    </Grid>
</Window>
